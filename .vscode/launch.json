{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Webdriver",
      "type": "node",
      "request": "launch",
      "timeout": 20000,
      "program": "${workspaceRoot}/src/start.js",
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Debug Webdriver",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "address": "127.0.0.1",
      "port": 5859,
      "timeout": 20000,
      "program": "${workspaceRoot}/src/start.js",
      "cwd": "${workspaceRoot}",
      "args": ["-debug"]
    }
  ]
}
