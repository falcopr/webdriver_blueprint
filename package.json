{
  "name": "webdriver_protractor_showcase",
  "version": "1.0.0",
  "description": "Showcasing webdriver usage with protractor",
  "main": "src/start.mjs",
  "scripts": {
    "preinstall-globals": "npm install --global --production windows-build-tools",
    "start": "node src/start.js",
    "debug": "node src/start.js -debug",
    "selenium": "java \"-Dwebdriver.gecko.driver=./bin/geckodriver-v0.22.0-win64/geckodriver.exe\" \"-Dwebdriver.chrome.driver=./bin/chromedriver-v2.42_win32/chromedriver.exe\" \"-Dwebdriver.ie.driver=./bin/IEDriverServer_x64_3.14.0/IEDriverServer.exe\" -jar ./bin/selenium-server-standalone-3.14.0.jar -port 5555",
    "report": ".\\node_modules\\.bin\\allure generate ./allure-results -o ./allure-reports --clean",
    "repl": ".\\node_modules\\.bin\\wdio repl firefox --port 5555",
    "watch": ".\\node_modules\\.bin\\wdio src/wdio.conf.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/falcoprescher/webdriver_protractor_showcase.git"
  },
  "keywords": [
    "Protractor",
    "Selenium",
    "Webdriver"
  ],
  "author": "Falco Prescher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/falcoprescher/webdriver_protractor_showcase/issues"
  },
  "homepage": "https://github.com/falcoprescher/webdriver_protractor_showcase#readme",
  "dependencies": {
    "allure-commandline": "^2.7.0",
    "jasmine": "^3.2.0",
    "jasmine-spec-reporter": "^4.2.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "protractor": "^5.4.1",
    "webdriverio": "^4.13.2"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-webdriverio": "^1.0.1",
    "wdio-allure-reporter": "^0.7.0",
    "wdio-dot-reporter": "0.0.10",
    "wdio-jasmine-framework": "^0.3.6",
    "wdio-spec-reporter": "^0.1.5",
    "yargs": "^12.0.2"
  }
}
